{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:local": "nest start --watch",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch --runInBand --verbose --detectOpenHandles",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy:demointernal": "node deploy.demo.js internal",
    "deploy:demoexternal": "node deploy.demo.js external",
    "deploy:stag": "./deploy.staging.sh",
    "deploy:prev": "./deploy.preview.sh",
    "deploy:prod": "./deploy.my.production.sh",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build --force-recreate",
    "docker:local": "docker-compose -f docker-compose.local.yml up",
    "rimraf": "./node_modules/rimraf/bin.js"
  },
  "dependencies": {
    "@azure/msal-node": "^1.3.3",
    "@microsoft/microsoft-graph-types": "^2.19.0",
    "@nestjs/apollo": "^10.0.8",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.4.3",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.3",
    "@nestjs/graphql": "^10.0.8",
    "@nestjs/mongoose": "^9.0.0",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.4.3",
    "@octokit/core": "^3.5.1",
    "@types/dotenv": "^8.2.0",
    "@types/lodash": "^4.14.180",
    "apollo-server-express": "^3.6.6",
    "aws-sdk": "^2.893.0",
    "cache-manager": "^3.6.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dataloader": "^2.0.0",
    "date-fns": "^2.18.0",
    "dayjs": "^1.10.5",
    "dotenv": "^8.2.0",
    "got": "^11.8.3",
    "graphql": "^16.3.0",
    "graphql-type-json": "^0.3.2",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "lru-cache": "^7.8.1",
    "mongoose": "^6.2.0",
    "passport": "^0.4.1",
    "passport-azure-ad": "^4.3.1",
    "performant-array-to-tree": "^1.8.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/cache-manager": "^3.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.0",
    "@types/lru-cache": "^7.6.1",
    "@types/node": "^16.0.0",
    "@types/passport-azure-ad": "^4.3.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
